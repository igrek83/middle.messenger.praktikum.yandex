{
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "./src/",
    "target": "es2016",
    "declaration": false,
    "module": "ES6",
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types", "typings/app.d.ts"]
  }
}
